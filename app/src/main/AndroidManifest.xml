<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.danbilap.project_yeobo">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <action android:name="com.google.android.gms.actions.SEARCH_ACTION"/>
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:name="com.poster.danbilap.project_yeobo.MyApp">

    <activity android:name="com.poster.danbilap.project_yeobo.BootActivity"
        android:launchMode="singleTask">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.PICK"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        <intent-filter>
            <action android:name="android.intent.action.SEND" />
            <category android:name="android.intent.category.DEFAULT" />
            <data android:mimeType="image/*" />
            <data android:mimeType="video/*"/>
        </intent-filter>
        <intent-filter>
            <action android:name="android.intent.action.SEND" />
            <category android:name="android.intent.category.DEFAULT" />
            <data android:mimeType="text/plain" />
        </intent-filter>

        </activity>

        <activity android:name="com.poster.danbilap.project_yeobo.LoginActivity"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.Join"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.MainActivity"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.SecondActivity"> </activity>
        <activity android:name="com.poster.danbilap.project_yeobo.AddActivity"> </activity>
        <activity android:name="com.poster.danbilap.project_yeobo.Side_1"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.Tour_info"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.MemoActivity"></activity>
        <activity android:name="com.poster.danbilap.project_yeobo.WriteActivity"></activity>

    </application>

</manifest>
